{
    "name": "gallib/laravel-short-url",
    "description": "A Laravel package to shorten urls",
    "keywords": [
        "laravel",
        "package",
        "url",
        "shortener"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alain Pellaux",
            "email": "alain@gallib.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/auth": "^10.0",
        "illuminate/contracts": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/support": "^10.0",
        "guzzlehttp/guzzle": "^7.4",
        "symfony/dom-crawler": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.5.10",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Gallib\\ShortUrl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gallib\\ShortUrl\\ShortUrlServiceProvider"
            ],
            "aliases": {
                "Hasher": "Gallib\\ShortUrl\\Facades\\Hasher",
                "ShortUrl": "Gallib\\ShortUrl\\Facades\\ShortUrl"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
